{{yield (hash
  media=(component "paper-swiper/media")
  headline=(component "paper-swiper/headline" destinationId=headlineId)
  subhead=(component "paper-swiper/subhead" destinationId=subheadId)
)}}

<div class="paper-swiper-text">
  <div id={{headlineId}} class="paper-swiper-headline"></div>
  <div id={{subheadId}} class="paper-swiper-subhead"></div>
  <div class="paper-swiper-fixed-content-placeholder"></div>
</div>